{
  "name": "react-painter",
  "version": "1.0.0",
  "description": "test",
  "repository": {
    "type": "git",
    "url": "https://github.com/marborkowski/react-painter"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marborkowski/react-painter/issues"
  },
  "homepage": "",
  "author": "Marcin Borkowski",
  "keywords": [
    "react",
    "canvas",
    "canvas draw",
    "paint",
    "react-component"
  ],
  "scripts": {
    "build": "babel --plugins 'transform-es2015-modules-umd' src --ignore __tests__ --out-dir ./dist",
    "test": "jest ./__tests__/*.test.js --verbose",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "globals": {
      "widow": true,
      "self": true
    },
    "automock": false
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^3.2.2",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^4.3.0",
    "jest": "^18.1.0",
    "jsdom": "^8.1.0",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-test-renderer": "^15.4.0"
  },
  "peerDependencies": {
    "react": "~0.14.8 || ^15.0.0",
    "react-dom": "~0.14.8 || ^15.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "react": "^15.0.0"
  }
}
